<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="SettingsItem">
        <attr name="leftTitle" format="string" />
        <attr name="contentTitle" format="string" />
        <attr name="rightTitle" format="string" />
        <attr name="isShowButton" format="boolean" />
        <attr name="isShowImage" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UserItem">
        <attr name="user_leftTitle" format="string" />
        <attr name="user_isShowImage" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StickyLinearLayout_Layout" tools:ignore="ResourceName">
        <attr name="isSticky" format="boolean" />
    </declare-styleable>
</resources>